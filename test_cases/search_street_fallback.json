{
  "name": "search street fallback",
  "priorityThresh": 5,
  "endpoint": "search",
  "description": [
    "query should return street centroid(s) followed by one or",
    "more addresses on that street."
  ],
  "tests": [
    {
      "id": 1,
      "status": "pass",
      "user": "missinglink",
      "issue": "https://github.com/pelias/openstreetmap/issues/28",
      "in": {
        "text": "grolmanstraße, berlin"
      },
      "expected": {
        "coordinates": [ 13.324042, 52.503915 ],
        "properties": [
          {
            "country_a": "DEU",
            "street": "Grolmanstraße",
            "layer": "street"
          },{
            "country_a": "DEU",
            "street": "Grolmanstraße",
            "layer": "address"
          }
        ]
      }
    },{
      "id": 2,
      "status": "pass",
      "user": "missinglink",
      "in": {
        "text": "glen rd, kelburn"
      },
      "expected": {
        "coordinates": [ 174.76323, -41.287732 ],
        "properties": [
          {
            "country_a": "NZL",
            "street": "Glen Road",
            "layer": "street"
          },{
            "country_a": "NZL",
            "street": "Glen Road",
            "layer": "address"
          }
        ]
      }
    }
  ]
}
